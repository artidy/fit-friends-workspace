{
  "name": "gym",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/gym/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/gym",
        "main": "apps/gym/src/main.ts",
        "tsConfig": "apps/gym/tsconfig.app.json",
        "assets": ["apps/gym/src/assets"],
        "isolatedConfig": true,
        "webpackConfig": "apps/gym/webpack.config.js"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "gym:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "gym:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/gym/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/gym/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "db-validate": {
    "executor": "nx:run-commands",
    "options": {
      "command": "npx prisma validate --schema ./prisma/schema.prisma",
      "cwd": "apps/gym"
    }
  },
  "db-migrate": {
    "executor": "nx:run-commands",
    "options": {
      "command": "npx prisma migrate dev --schema ./prisma/schema.prisma --name initial --skip-generate --skip-seed",
      "cwd": "apps/gym"
    }
  },
  "db-reset": {
    "executor": "nx:run-commands",
    "options": {
      "command": "npx prisma migrate reset --schema ./prisma/schema.prisma --force --skip-generate --skip-seed",
      "cwd": "apps/gym"
    }
  },
  "db-generate": {
    "executor": "nx:run-commands",
    "options": {
      "command": "npx prisma generate --schema ./prisma/schema.prisma",
      "cwd": "apps/gym"
    }
  },
  "db-fill": {
    "executor": "nx:run-commands",
    "options": {
      "command": "npx prisma db seed",
      "cwd": "apps/gym"
    }
  },
  "tags": []
}
